{
	"name": "drive-by",
	"displayName": "Drive By",
	"description": "VS Code plugin that records and plays back coding sessions",
	"version": "0.0.16",
	"publisher": "airportyh",
	"enableProposedApi": true,
	"repository": {
		"type": "git",
		"url": "https://github.com/airportyh/drive-by.git"
	},
	"engines": {
		"vscode": "^1.25.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"views": {
			"explorer": [
				{
					"id": "driveBy",
					"name": "Drive By"
				}
			]
		},
		"commands": [
			{
				"command": "driveBy.start",
				"title": "Start"
			},
			{
				"command": "driveBy.stop",
				"title": "Stop"
			},
			{
				"command": "driveBy.restore",
				"title": "Restore"
			},
			{
				"command": "driveBy.next",
				"title": "Next"
			},
			{
				"command": "driveBy.previous",
				"title": "Previous"
			},
			{
				"command": "driveBy.beginSection",
				"title": "Begin Section"
			},
			{
				"command": "driveBy.toggleSections",
				"title": "Toggle Sections"
			},
			{
				"command": "driveBy.copyCommitSha",
				"title": "Copy Commit Sha"
			},
			{
				"command": "driveBy.reset",
				"title": "Reset"
			},
			{
				"command": "driveBy.branchHere",
				"title": "Branch Here..."
			},
			{
				"command": "driveBy.switchBranch",
				"title": "Switch Branch..."
			},
			{
				"command": "driveBy.revertToCommit",
				"title": "Revert to this Commit"
			}
		],
		"menus": {
			"view/item/context": [
				{
					"command": "driveBy.restore",
					"group": "inline",
					"when": "viewItem == commit"
				},
				{
					"command": "driveBy.beginSection",
					"when": "viewItem == commit"
				},
				{
					"command": "driveBy.copyCommitSha",
					"when": "viewItem == commit"
				},
				{
					"command": "driveBy.branchHere",
					"when": "viewItem == commit"
				},
				{
					"command": "driveBy.revertToCommit",
					"when": "viewItem == commit"
				}
			],
			"view/title": [
				{
					"command": "driveBy.stop",
					"when": "view == driveBy"
				},
				{
					"command": "driveBy.toggleSections",
					"when": "view == driveBy"
				},
				{
					"command": "driveBy.switchBranch",
					"when": "view == driveBy"
				}
			]
		},
		"keybindings": [
			{
				"command": "driveBy.next",
				"key": "alt-ctrl-right"
			},
			{
				"command": "driveBy.next",
				"key": "pagedown"
			},
			{
				"command": "driveBy.previous",
				"key": "alt-ctrl-left"
			},
			{
				"command": "driveBy.previous",
				"key": "pageup"
			},
			{
				"command": "driveBy.reset",
				"key": "alt-ctrl-space"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"package": "vsce package"
	},
	"devDependencies": {
		"@types/mkdirp": "^0.5.2",
		"@types/node": "^14.0.1",
		"@types/rimraf": "^2.0.2",
		"tslint": "^5.11.0",
		"typescript": "^2.1.4",
		"vsce": "^1.57.1",
		"vscode": "^1.1.36"
	},
	"dependencies": {
		"@types/diff": "^3.5.2",
		"@types/lodash": "^4.14.119",
		"@types/mkdirp-promise": "^5.0.0",
		"@types/mz": "0.0.32",
		"@types/rmfr": "^2.0.0",
		"@types/sha1": "^1.1.1",
		"diff": "^4.0.1",
		"lodash": "^4.17.13",
		"mkdirp-promise": "^5.0.1",
		"moment": "^2.23.0",
		"mz": "^2.7.0",
		"rmfr": "^2.0.0",
		"rxjs": "^6.4.0",
		"sha1": "^1.1.1"
	}
}
